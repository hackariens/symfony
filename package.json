  "description": "Template de site internet sous Symfony",
  "main": "index.js",
  "scripts": {
    "create:apps": "docker container run -v $(pwd)/:/var/www --user $(id -u):$(id -g) $(npx korojscommands docker_get-image --key phpfpm) composer create-project symfony/skeleton:\"6.2.*\" apps --no-interaction",
    "encore:build": "encore production --progress",
    "encore:dev-server": "encore dev-server",
    "encore:dev": "encore dev",
    "encore:watch": "encore dev --watch",
